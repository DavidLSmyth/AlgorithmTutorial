<h1><center>Algorithm Listing</center></h1>
<% if @algorithms.empty? %>
	<center>No Algorithms found</center>
<% else %>
	<% @algorithms.each do |alg| %>
		  <br>
			<center>
		  <h2><%= link_to alg.name, alg %></h2>
		  <%= sanitize alg.purpose %>
		  <h3> Difficulty: <span><%= alg.difficulty %></span></h3>
			</center>
		  </br>
	<% end %>
<% end %>

